{
  "manifest_version": 2,
  "name": "__MSG_extName__",
  "homepage_url": "http://localhost:8080/",
  "description": "Dict Words",
  "default_locale": "en",
  "permissions": [
    "activeTab",
    "<all_urls>",
    "*://*/*"
  ],
  "icons": {
    "16": "icons/16.png",
    "48": "icons/48.png",
    "128": "icons/128.png"
  },
  "background": {
    "scripts": [
      "js/background.js"
    ],
    "persistent": false
  },
  "content_scripts": [
    {
      "js": [
        "js/content-script.js"
      ],
      "css": [
        "css/index.css"
      ],
      "matches": [
        "http://localhost/popup_main.html*",
        "https://image.baidu.com/*"
      ],
      "all_frames": true
    }
  ],
  "browser_action": {
    "default_title": "__MSG_extName__",
    "default_popup": "popup.html",
    "default_icon": {
      "19": "icons/19.png",
      "38": "icons/38.png"
    }
  },
  "version": "0.1.0",
  "content_security_policy": "script-src 'self' 'unsafe-eval' ; object-src 'self'"
}